-- main : {List Char}
-- main! = "abc"

ex : Char -> [Exception Unit]Char
ex n = if (eqChar n 'a') {throw unit} {'c'}

avc : {[]Char}
avc! = {'c'}

main : {Either Unit Char}
main! = catch (ex 'b')

